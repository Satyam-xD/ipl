{
  "version": 2,
  "public": true,
  "rewrites": [
    { "source": "/api/scores", "destination": "/api/scores.js" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "s-maxage=30" }
      ]
    }
  ]
}
